{
  "name": "ngx-breadcrumbs-light",
  "version": "1.0.2",
  "description": "Angular Breadcrumbs customizable and easy to use component & service",
  "scripts": {
    "ng": "ng",
    "copy": ". ./copy.sh",
    "start": "ng serve",
    "base": "ng serve --base-href=\"/angular-breadcrumbs-light/\"",
    "build:demo": "ng build --prod --base-href=\"https://mopcweb.github.io/angular-breadcrumbs-light/\"",
    "build:lib": "npm run copy && ng build AngularBreadcrumbsLight",
    "test": "ng test",
    "lint": "tslint -p tsconfig.json",
    "e2e": "ng e2e",
    "deploy": "npm run copy && npm run build:demo && ngh --dir dist/ngx-breadcrumbs-light",
    "release": "npm run build:lib && cd dist/angular-breadcrumbs-light && npm publish"
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "^7.2.15",
    "@angular/cdk": "^7.3.7",
    "@angular/common": "~7.2.0",
    "@angular/compiler": "~7.2.0",
    "@angular/core": "~7.2.0",
    "@angular/forms": "~7.2.0",
    "@angular/material": "^7.3.7",
    "@angular/platform-browser": "~7.2.0",
    "@angular/platform-browser-dynamic": "~7.2.0",
    "@angular/router": "~7.2.0",
    "angular-breadcrumbs-light": "^1.0.2",
    "axios": "^0.21.1",
    "core-js": "^2.5.4",
    "hammerjs": "^2.0.8",
    "ngx-cookie-service": "^2.2.0",
    "rxjs": "~6.3.3",
    "tslib": "^1.9.0",
    "zone.js": "~0.8.26"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "~0.13.0",
    "@angular-devkit/build-ng-packagr": "~0.13.0",
    "@angular/cli": "~7.3.9",
    "@angular/compiler-cli": "~7.2.0",
    "@angular/language-service": "~7.2.0",
    "@types/jasmine": "~2.8.8",
    "@types/jasminewd2": "~2.0.3",
    "@types/node": "~8.9.4",
    "angular-cli-ghpages": "^0.5.3",
    "codelyzer": "~4.5.0",
    "jasmine-core": "~2.99.1",
    "jasmine-spec-reporter": "~4.2.1",
    "karma": "~4.0.0",
    "karma-chrome-launcher": "~2.2.0",
    "karma-coverage-istanbul-reporter": "~2.0.1",
    "karma-jasmine": "~1.1.2",
    "karma-jasmine-html-reporter": "^0.2.2",
    "ng-packagr": "^4.2.0",
    "protractor": "~5.4.0",
    "ts-node": "~7.0.0",
    "tsickle": ">=0.34.0",
    "tslib": "^1.9.0",
    "tslint": "~5.11.0",
    "typescript": "~3.2.2"
  },
  "keywords": [
    "Angular",
    "Breadcrumbs",
    "angular-breadcrumbs"
  ],
  "author": "Daniil Moroz",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/mopcweb/angular-breadcrumbs-light"
  },
  "homepage": "https://mopcweb.github.io/angular-breadcrumbs-light"
}
